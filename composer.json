  "require": {
    "docopt/docopt": "^1.0",
    "phpunit/phpunit": "^9.6"
  },
  "bin": [
    "bin/gendiff"
  ],
  "require-dev": {
    "docopt/docopt": "^1.0",
    "squizlabs/php_codesniffer": "*",
    "phpunit/phpunit": "^9.6"
  }
}
